{
    "name": "fansipan/mist",
    "description": "Fansipan SDK generator from OpenAPI spec",
    "keywords": [
        "fansipan",
        "sdk",
        "openapi",
        "generator"
    ],
    "homepage": "https://github.com/phanxipang/mist",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "amphp/parallel": "^2.2",
        "amphp/pipeline": "^1.1",
        "archtechx/enums": "^1.0",
        "beberlei/assert": "^3.3",
        "cebe/php-openapi": "^1.7",
        "composer/semver": "^3.4",
        "cuyz/valinor": "^1.8",
        "fansipan/fansipan": "^1.0",
        "illuminate/view": "^10.0",
        "laravel-zero/framework": "^10.2",
        "nette/php-generator": "^3.6|^4.0",
        "nunomaduro/termwind": "^1.15.1",
        "symfony/filesystem": "^7.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Fansipan\\Mist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fansipan\\Mist\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse --memory-limit=-1"
        ],
        "cs": [
            "vendor/bin/pint"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage"
        ],
        "check": [
            "@cs --test",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["mist"]
}
